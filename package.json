{
  "name": "eliashomesltd",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "pnpm run --parallel /^dev:.*/",
    "dev:server": "TZ=UTC vite",
    "dev:check:types": "tsc --watch",
    "build": "tsc -b && vite build",
    "format": "prettier . --write --list-different",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate": "tanstack-router generate",
    "watch": "tanstack-router watch",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.99.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.6",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "^9.20.0",
    "@tanstack/router-devtools": "^1.99.13",
    "@tanstack/router-plugin": "^1.99.14",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-check-file": "^3.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "3.5.0",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown --cache --list-different"
  }
}
